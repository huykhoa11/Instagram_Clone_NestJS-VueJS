     followerId: followerId,\n          followingId: followingId\n        };\n        const response = await follow(data);\n        followBtnElement.innerHTML = 'Following';\n        followBtnElement.classList.remove('text-white', 'bg-blue-400');\n        followBtnElement.classList.add('text-gray-700', 'border', 'border-gray-300');\n      } catch (error) {\n        followBtnElement.innerHTML = 'Follow';\n        displayToast(\"Error happend, can't follow user\", dangerColor);\n      }\n    };\n\n    onMounted(async () => {\n      // const userId = parseInt(router.currentRoute.value.params.id);\n      console.log('inside inmounted user page');\n      const userId = router.currentRoute.value.params.id;\n      currentUserId.value = parseInt(router.currentRoute.value.query.currentUserId);\n\n      try {\n        let response = await axios.get('http://localhost:3000/users/' + userId, {\n          withCredentials: true\n        });\n        user.value = response.data;\n        user.value.tasks.forEach(element => {\n          open.value[`${element.id}`] = false;\n        });\n        console.log(typeof user.value.id);\n        console.log(typeof currentUserId.value);\n        console.log('user.id', user.value.id, ' currentUserId', currentUserId.value);\n        relation.value = await getRelation(currentUserId.value, userId);\n        console.log(relation.value);\n        response = await axios.get('http://localhost:3000/follows/user/' + userId, {\n          withCredentials: true\n        });\n        const {\n          numFollowers,\n          numFollowing\n        } = response.data;\n        followers.value = numFollowers;\n        following.value = numFollowing; // response = await \n      } catch (error) {\n        console.log(error);\n\n        if (error.response.status === 401) {\n          router.push('/auth/signin');\n          localStorage.removeItem('username');\n        }\n      }\n    });\n    const __returned__ = {\n      router,\n      user,\n      currentUserId,\n      relation,\n      followers,\n      following,\n      open,\n      followEvent,\n      removeRelation,\n      addRelation,\n      ref,\n      onMounted,\n      computed,\n      useRouter,\n      axios,\n      displayToast,\n      dangerColor,\n      successColor,\n      follow,\n      deleteFollow,\n      getRelation,\n      spin,\n      Task\n    };\n    Object.defineProperty(__returned__, '__isScriptSetup', {\n      enumerable: false,\n      value: true\n    });\n    return __returned__;\n  }\n\n};","map":{"version":3,"mappings":";AA2FA,SAASA,GAAT,EAAcC,SAAd,EAAyBC,QAAzB,QAAyC,KAAzC;AACA,SAASC,SAAT,QAA0B,YAA1B;AACA,OAAOC,KAAP,MAAkB,OAAlB;AACA,SAASC,YAAT,EAAuBC,WAAvB,EAAoCC,YAApC,QAAwD,kCAAxD;AACA,SAASC,MAAT,EAAiBC,YAAjB,EAA+BC,WAA/B,EAA4CC,IAA5C,QAAwD,2BAAxD;AACA,OAAOC,IAAP,MAAiB,YAAjB;;;;;;;;IAEA,MAAMC,MAAM,GAAGV,SAAS,EAAxB;IACA,MAAMW,IAAI,GAAGd,GAAG,CAAC,IAAD,CAAhB;IACA,MAAMe,aAAa,GAAGf,GAAG,CAAC,IAAD,CAAzB;IACA,MAAMgB,QAAQ,GAAGhB,GAAG,CAAC,IAAD,CAApB;IACA,MAAMiB,SAAS,GAAGjB,GAAG,CAAC,IAAD,CAArB;IACA,MAAMkB,SAAS,GAAGlB,GAAG,CAAC,IAAD,CAArB;IACA,MAAMmB,IAAI,GAAGnB,GAAG,CAAC,EAAD,CAAhB,EAEA;;IAEA,MAAMoB,WAAW,GAAG,OAAMC,UAAN,EAAkBC,WAAlB,KAAkC;MAClD,MAAMC,gBAAgB,GAAGC,QAAQ,CAACC,cAAT,CAAwB,WAAxB,CAAzB;MACA,MAAMC,GAAG,GAAGH,gBAAgB,CAACI,SAA7B;;MAEA,IAAGX,QAAQ,CAACY,KAAZ,EAAmB;QAACC,cAAc,CAACb,QAAQ,CAACY,KAAV,EAAiBL,gBAAjB,CAAd;MAAkD,CAAtE,MACK;QAACO,WAAW,CAACT,UAAD,EAAaC,WAAb,EAA0BC,gBAA1B,CAAX;MAAwD;IACjE,CAND;;IAQA,MAAMM,cAAc,GAAG,OAAMb,QAAN,EAAgBO,gBAAhB,KAAqC;MACxDQ,OAAO,CAACC,GAAR,CAAY,mBAAZ;MACAD,OAAO,CAACC,GAAR,CAAYhB,QAAZ;MACAO,gBAAgB,CAACI,SAAjB,GAA6BhB,IAAI,CAAC,MAAD,CAAjC;;MACA,IAAI;QACA,MAAMsB,QAAQ,GAAG,MAAMxB,YAAY,CAACO,QAAQ,CAACkB,EAAV,CAAnC;QACAX,gBAAgB,CAACI,SAAjB,GAA6B,QAA7B;QACAJ,gBAAgB,CAACY,SAAjB,CAA2BC,MAA3B,CAAkC,eAAlC,EAAmD,QAAnD,EAA6D,iBAA7D;QACAb,gBAAgB,CAACY,SAAjB,CAA2BE,GAA3B,CAA+B,YAA/B,EAA6C,aAA7C;MACH,CALD,CAKE,OAAOC,KAAP,EAAc;QACZf,gBAAgB,CAACI,SAAjB,GAA6B,WAA7B;QACAtB,YAAY,CAAC,mCAAD,EAAsCC,WAAtC,CAAZ;MACH;IACJ,CAbD;;IAeA,MA